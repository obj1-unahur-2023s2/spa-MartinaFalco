import Personas.*

test "Ramiro tiene la piel grasosa al volver del trabajo" {
	ramiro.diaDeTrabajo()
	assert.that(ramiro.tieneLaPielGrasosa())
	
}

test "Ramiro no tiene la piel grasosa luego de tomar un baño de vapor" {
	ramiro.tomarBanioDeVapor()
	assert.notThat(ramiro.tieneLaPielGrasosa())
	
}

